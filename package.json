{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "limdu",
    "description": "api documentation for [limdu (v0.9.2)](https://github.com/erelsgl/limdu#readme) [![npm package](https://img.shields.io/npm/v/npmdoc-limdu.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-limdu) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-limdu.svg)](https://travis-ci.org/npmdoc/node-npmdoc-limdu)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "limdu": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-limdu",
    "keywords": [
        "documentation",
        "limdu"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_limdu.js",
    "name": "npmdoc-limdu",
    "nameAlias": "npmdoc_limdu",
    "nameOriginal": "npmdoc-limdu",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-limdu"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
